   "source": [
    "a=str(input(\"enter a word\"))\n",
    "l=[]\n",
    "l1=[]\n",
    "s=len(a)\n",
    "for i in range(0,len(a)):\n",
    "    l.append(a[i])\n",
    "for j in range(s-1,-1,-1):\n",
    "    l1.append(a[j])\n",
    "\n",
    "\n",
    "if l==l1:\n",
    "    print(a,\"is a palindrome\")\n",
    "else:\n",
    "    print(a,\"is not a palindrome\")\n",
    "\n",
    "\n",
    "    \n"
   ]
